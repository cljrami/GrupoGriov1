import "../../src/estilos/global.css";
import LayoutBase from "../layouts/LayoutBase.astro";
//SLIDER
import SliderServicios from "../components/SwiperSliders/SliderServicios";
---

<LayoutBase
  siteName={"Le Website"}
  title="Grupo de Rescate Integral Organizado"
  titleDefault="Tinker Tailor Soldier Spy"
  description="Agent"
  canonical="https://www.emol.com"
  charset="utf-8"
